<?xml version="1.0"?>
<block>
  <name>oqpsk_phase_clock_est_cc</name>
  <key>oqpsk_oqpsk_phase_clock_est_cc</key>
  <category>oqpsk</category>
  <import>import oqpsk</import>
  <make>oqpsk.oqpsk_phase_clock_est_cc($sps, $taps)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Samples Per Symbol</name>
    <key>sps</key>
    <type>real</type>
  </param>
  <param>
    <name>Filter Taps</name>
    <key>taps</key>
    <type>real_vector</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <!-- Optional Outputs -->
  <source>
    <name>phase</name>
    <type>float</type>
    <optional>1</optional>
  </source>
  <source>
    <name>delay</name>
    <type>float</type>
    <optional>1</optional>
  </source>
</block>
